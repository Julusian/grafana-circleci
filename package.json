{
  "name": "grafana-gitlab-for-github",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "del dist && yarn build:main",
    "build:main": "tsc -p tsconfig.build.json",
    "lint": "eslint . --ext .ts --ignore-pattern dist",
    "dev": "yarn ts-node src/index.ts"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/node": "^14.14.7",
    "@types/underscore": "^1.10.24",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "del-cli": "^3.0.1",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "prettier": "^2.1.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  },
  "engines": {
    "node": "^12.16",
    "yarn": "^1.22"
  },
  "dependencies": {
    "@godaddy/terminus": "^4.4.1",
    "axios": "^0.21.0",
    "express": "^4.17.1",
    "mysql2": "^2.2.5",
    "p-queue": "^6.6.2",
    "reissue": "^3.0.0",
    "sequelize": "^6.3.5",
    "underscore": "^1.11.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write"
    ],
    "*.{ts}": [
      "yarn lint:fix --"
    ]
  }
}
